{
    "name": "ezchat-api",
    "version": "1.0.0",
    "description": "EzChat Appliation Backend API Services",
    "main": "index.ts",
    "scripts": {
        "dev": "env-cmd --file .env.development ts-node-dev --transpile-only --ignore-watch node_modules src/index.ts",
        "test": "env-cmd --file .env.test jest --silent --runInBand --detectOpenHandles --passWithNoTests",
        "test:watch": "env-cmd --file .env.test jest --watch",
        "build": "npx tsc",
        "start:production": "env-cmd --file .env.production npm run build && node ./build/index.js",
        "start:development": "ts-node-dev src/swagger/swagger.ts && env-cmd --file .env.development ts-node-dev --transpile-only --ignore-watch node_modules src/index.ts",
        "knex": "env-cmd --file .env.development knex --knexfile src/database/knexfile.ts",
        "knex:migrate:latest": "env-cmd --file .env.development knex --knexfile src/database/knexfile.ts migrate:latest",
        "knex:migrate:rollback": "env-cmd --file .env.development npm run knex migrate:rollback",
        "knex:seed": "env-cmd --file .env.development knex --knexfile src/database/knexfile.ts seed:run",
        "prisma:dev:sync": "env-cmd --file .env.development prisma db pull && prisma generate && prisma format",
        "prisma:prod:sync": "env-cmd --file .env.production prisma db pull && prisma generate && prisma format",
        "prettier-format": "prettier --config .prettierrc.json 'src/**/*.ts' --write",
        "format": "npx prettier --write \"src/**/*.ts\"",
        "format-all": "npx prettier --write .",
        "lint": "npx eslint .",
        "swagger": "ts-node src/swagger/swagger.ts",
        "start:cron-dev": "env-cmd --file .env.development ts-node src/cron/index.ts",
        "start:cron-prod": "env-cmd --file .env.production ts-node src/cron/index.ts"
    },
    "keywords": [
        "ezchat",
        "typescript",
        "docker",
        "waha"
    ],
    "author": "jktan0504@hotmail.com",
    "license": "ISC",
    "devDependencies": {
        "@faker-js/faker": "^8.4.1",
        "@swc/cli": "^0.3.12",
        "@swc/core": "^1.4.17",
        "@types/bcrypt": "^5.0.2",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/express-fileupload": "^1.5.0",
        "@types/express-pino-logger": "^4.0.5",
        "@types/jest": "^29.5.12",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/node": "^20.12.7",
        "@types/node-cron": "^3.0.11",
        "@types/redis": "^4.0.11",
        "@types/redis-mock": "^0.17.3",
        "@types/uuid": "^9.0.8",
        "@typescript-eslint/eslint-plugin": "^7.8.0",
        "@typescript-eslint/parser": "^7.8.0",
        "env-cmd": "^10.1.0",
        "eslint": "^8.57.0",
        "express-pino-logger": "^7.0.0",
        "jest": "^29.7.0",
        "moment-timezone": "^0.5.45",
        "nock": "^13.5.4",
        "nodemon": "^3.1.0",
        "prisma": "^5.13.0",
        "redis-mock": "^0.56.3",
        "rimraf": "^5.0.5",
        "swagger-autogen": "^2.23.7",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.4.5"
    },
    "dependencies": {
        "@prisma/client": "^5.13.0",
        "aws-sdk": "^2.1609.0",
        "axios": "^1.6.8",
        "bcrypt": "^5.1.1",
        "cors": "^2.8.5",
        "csvtojson": "^2.0.10",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-async-error": "^0.0.2",
        "express-async-errors": "^3.1.1",
        "express-fileupload": "^1.5.0",
        "fast-equals": "^5.0.1",
        "http-status-codes": "^2.3.0",
        "inversify": "^6.0.2",
        "joi": "^17.13.0",
        "jsonwebtoken": "^9.0.2",
        "knex": "^3.1.0",
        "knex-paginate": "^3.1.2",
        "moment": "^2.30.1",
        "nanoid": "^5.0.7",
        "node-cron": "^3.0.3",
        "pg": "^8.11.5",
        "pino": "^9.0.0",
        "pino-http": "^9.0.0",
        "pino-pretty": "^11.0.0",
        "redis": "^4.6.13",
        "reflect-metadata": "^0.2.2",
        "uuid": "^9.0.1"
    }
}
